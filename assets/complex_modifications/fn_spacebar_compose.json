{
  "title": "fn spacebar compose",
          "rules":[
             {
                  "manipulators":[
                      {
                          "description":"fn + spacebar = non_us_backslash",
                          "type":"basic",
                          "from":{
                              "key_code":"spacebar",
                              "modifiers":{ "mandatory":["fn"]}
                          },
                          "to":[{"key_code":"non_us_backslash"}]
                      }
                  ]
              }
          ]
        
      }