{
  "title": "qwerty arrow keys",
          "rules":[
             {
                  "manipulators":[
                      {
                          "description":"fn + f = RIGHT",
                          "type":"basic",
                          "from":{
                              "key_code":"f",
                              "modifiers":{ "mandatory":["fn"]}
                          },
                          "to":[{"key_code":"right_arrow"}]
                      }
                  ]
              },{
                  "manipulators":[
                      {
                          "description":"fn + d = DOWN",
                          "type":"basic",
                          "from":{
                              "key_code":"d",
                              "modifiers":{ "mandatory":["fn"]}
                          },
                          "to":[{"key_code":"down_arrow"}]
                      }
                  ]
              },{
                  "manipulators":[
                      {
                          "description":"fn + s = Left",
                          "type":"basic",
                          "from":{
                              "key_code":"s",
                              "modifiers":{ "mandatory":["fn"]}
                          },
                          "to":[{"key_code":"left_arrow"}]
                      }
                  ]
              },{
                  "manipulators":[
                      {
                          "description":"fn + e = UP",
                          "type":"basic",
                          "from":{
                              "key_code":"e",
                              "modifiers":{ "mandatory":["fn"]}
                          },
                          "to":[{"key_code":"up_arrow"}]
                      }
                  ]
              },{
                  "manipulators":[
                      {
                          "description":"fn + a = HOME",
                          "type":"basic",
                          "from":{
                              "key_code":"a",
                              "modifiers":{ "mandatory":["fn"]}
                          },
                          "to": [
                              {
                                  "key_code": "left_arrow",
                                  "modifiers": ["right_command"]
                              }
                          ]
                      }
                  ]
              },{
                  "manipulators":[
                      {
                          "description":"fn + g = END",
                          "type":"basic",
                          "from":{
                              "key_code":"g",
                              "modifiers":{ "mandatory":["fn"]}
                          },
                          "to": [
                              {
                                  "key_code": "right_arrow",
                                  "modifiers": ["right_command"]
                              }
                          ]
                      }
                  ]
              }
          ]
        
      }