{
  "title": "qwerty arrow keys",
          "rules":[
             {
                  "manipulators":[
                      {
                          "description":"fn + f = RIGHT",
                          "type":"basic",
                          "from":{
                              "key_code":"f",
                              "modifiers":{ "mandatory":["fn"]}
                          },
                          "to":[{"key_code":"right_arrow"}]
                      }
                  ]
              },{
                  "manipulators":[
                      {
                          "description":"fn + d = DOWN",
                          "type":"basic",
                          "from":{
                              "key_code":"d",
                              "modifiers":{ "mandatory":["fn"]}
                          },
                          "to":[{"key_code":"down_arrow"}]
                      }
                  ]
              },{
                  "manipulators":[
                      {
                          "description":"fn + s = Left",
                          "type":"basic",
                          "from":{
                              "key_code":"s",
                              "modifiers":{ "mandatory":["fn"]}
                          },
                          "to":[{"key_code":"left_arrow"}]
                      }
                  ]
              },{
                  "manipulators":[
                      {
                          "description":"fn + e = UP",
                          "type":"basic",
                          "from":{
                              "key_code":"e",
                              "modifiers":{ "mandatory":["fn"]}
                          },
                          "to":[{"key_code":"up_arrow"}]
                      }
                  ]
              },{
                  "manipulators":[
                      {
                          "description":"fn + a = HOME non-iterm",
                          "type":"basic",
                          "from":{
                              "key_code":"a",
                              "modifiers":{ "mandatory":["fn"]}
                          },
                          "to": [
                              {
                                  "key_code": "left_arrow",
                                  "modifiers": ["right_command"]
                              }
                          ],
                          "conditions": [
                            {
                                "type": "frontmost_application_unless",
                                "bundle_identifiers": [
                                    "^com\\.apple\\.Terminal$",
                                    "^com\\.googlecode\\.iterm2$",
                                    "^org\\.xpra\\.xpra"
                                ]
                            }
                          ]
                      }
                  ]
              },
              {
                  "manipulators":[
                      {
                          "description":"fn + a = HOME iterm",
                          "type":"basic",
                          "from":{
                              "key_code":"a",
                              "modifiers":{ "mandatory":["fn"]}
                          },
                          "to": [
                              {
                                  "key_code": "home"
                              }
                          ],
                          "conditions": [
                            {
                                "type": "frontmost_application_if",
                                "bundle_identifiers": [
                                    "^com\\.apple\\.Terminal$",
                                    "^com\\.googlecode\\.iterm2$",
                                    "^org\\.xpra\\.xpra"
                                ]
                            }
                          ]
                      }
                  ]
              },
              {
                  "manipulators":[
                      {
                          "description":"fn + g = END non-iterm",
                          "type":"basic",
                          "from":{
                              "key_code":"g",
                              "modifiers":{ "mandatory":["fn"]}
                          },
                          "to": [
                              {
                                  "key_code": "right_arrow",
                                  "modifiers": ["right_command"]
                              }
                          ],
                          "conditions": [
                            {
                                "type": "frontmost_application_unless",
                                "bundle_identifiers": [
                                    "^com\\.apple\\.Terminal$",
                                    "^com\\.googlecode\\.iterm2$",
                                    "^org\\.xpra\\.xpra"
                                ]
                            }
                          ]
                      }
                  ]
              },
              {
                  "manipulators":[
                      {
                          "description":"fn + g = END iterm",
                          "type":"basic",
                          "from":{
                              "key_code":"g",
                              "modifiers":{ "mandatory":["fn"]}
                          },
                          "to": [
                              {
                                  "key_code": "end"
                              }
                          ],
                          "conditions": [
                            {
                                "type": "frontmost_application_if",
                                "bundle_identifiers": [
                                    "^com\\.apple\\.Terminal$",
                                    "^com\\.googlecode\\.iterm2$",
                                    "^org\\.xpra\\.xpra"
                                ]
                            }
                          ]
                      }
                  ]
              },
              {
                  "manipulators":[
                      {
                          "description":"HOME = cmd + left non-iterm",
                          "type":"basic",
                          "from":{
                              "key_code":"home"
                          },
                          "to": [
                              {
                                  "key_code": "left_arrow",
                                  "modifiers": ["right_command"]
                              }
                          ],
                          "conditions": [
                            {
                                "type": "frontmost_application_unless",
                                "bundle_identifiers": [
                                    "^com\\.apple\\.Terminal$",
                                    "^com\\.googlecode\\.iterm2$",
                                    "^org\\.xpra\\.xpra"
                                ]
                            }
                          ]
                      }
                  ]
              },
              {
                  "manipulators":[
                      {
                          "description":"END = cmd + right non-iterm",
                          "type":"basic",
                          "from":{
                              "key_code":"end"
                          },
                          "to": [
                              {
                                  "key_code": "right_arrow",
                                  "modifiers": ["right_command"]
                              }
                          ],
                          "conditions": [
                            {
                                "type": "frontmost_application_unless",
                                "bundle_identifiers": [
                                    "^com\\.apple\\.Terminal$",
                                    "^com\\.googlecode\\.iterm2$",
                                    "^org\\.xpra\\.xpra"
                                ]
                            }
                          ]
                      }
                  ]
              }
          ]
        
      }