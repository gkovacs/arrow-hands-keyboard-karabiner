{  
  "title":"qwerty punctuation",
  "rules":[  
    {  
      "manipulators":[  
        {
          "description":"fn + z = tilde",
          "type":"basic",
          "from":{  
            "key_code":"z",
            "modifiers":{  
              "mandatory":[  
                "fn"
              ]
            }
          },
          "to":[  
            {  
              "key_code":"grave_accent_and_tilde",
              "modifiers":[  
                "left_shift"
              ]
            }
          ]
        }
      ]
    },
    {  
      "manipulators":[  
        {  
          "description":"fn + x = plus",
          "type":"basic",
          "from":{  
            "key_code":"x",
            "modifiers":{  
              "mandatory":[  
                "fn"
              ]
            }
          },
          "to":[  
            {  
              "key_code":"keypad_plus"
            }
          ]
        }
      ]
    },
    {  
      "manipulators":[  
        {  
          "description":"fn + c = underscore",
          "type":"basic",
          "from":{  
            "key_code":"c",
            "modifiers":{  
              "mandatory":[  
                "fn"
              ]
            }
          },
          "to":[  
            {  
              "key_code":"hyphen",
              "modifiers":[  
                "left_shift"
              ]
            }
          ]
        }
      ]
    },
    {  
      "manipulators":[  
        {  
          "description":"fn + v = equals",
          "type":"basic",
          "from":{
            "key_code":"v",
            "modifiers":{  
              "mandatory":[  
                "fn"
              ]
            }
          },
          "to":[  
            {  
              "key_code":"equal_sign"
            }
          ]
        }
      ]
    },
    {  
      "manipulators":[  
        {  
          "description":"fn + b = minus",
          "type":"basic",
          "from":{
            "key_code":"b",
            "modifiers":{  
              "mandatory":[  
                "fn"
              ]
            }
          },
          "to":[  
            {
              "key_code":"hyphen"
            }
          ]
        }
      ]
    }
  ]
}