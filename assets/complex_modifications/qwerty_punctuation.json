      ]
    },
    {
      "manipulators":[  
        {  
          "description":"fn + p = close curly bracket",
          "type":"basic",
          "from":{
            "key_code":"p",
            "modifiers":{  
              "mandatory":[  
                "fn"
              ]
            }
          },
          "to":[  
            {  
